{
	"name": "@madisonbullard/mcp-server-tui",
	"version": "1.16.0",
	"license": "MIT",
	"access": "public",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "rm -rf dist && tsup src/cli.tsx --dts --format esm --external which",
		"postbuild": "shx chmod +x dist/cli.js",
		"dev": "tsc --watch",
		"typecheck": "tsc --noEmit"
	},
	"files": ["dist"],
	"dependencies": {
		"@inkjs/ui": "2.0.0",
		"ink": "5.2.0",
		"ink-link": "4.1.0",
		"react": "18.3.1",
		"which": "5.0.0"
	},
	"devDependencies": {
		"@types/react": "18.3.12",
		"@types/which": "3.0.4",
		"shx": "0.4.0",
		"tsup": "8.4.0"
	},
	"exports": {
		"import": {
			"types": "./dist/cli.d.ts",
			"default": "./dist/cli.js"
		}
	}
}
